<diagram dimensions="(300,300)" margins="5">
    <definition>f(x) = -3/4*x+4 </definition>
    <definition>g(x) = -3/4*x+2 </definition>
    <coordinates bbox="[-2,-1,5,6]">
        <axes at="axes" decorations="no"/>
        <graph at="line1" function='f' stroke="black" />
        <graph at="line2" function='g' stroke="black" />
    </coordinates>

    <annotations>
    <annotation ref="figure"
                text="The two-dimensional plane with axes and two parallel lines">
        <annotation ref="axes" text="The coordinate axes" />
        <annotation ref="parallel-lines" text="A pair of parallel lines">
          <annotation ref="line1" text="First of the two lines"/>
          <annotation ref="line2" text="Second of the two lines"/>
        </annotation>
      </annotation>
  </annotations>
</diagram>