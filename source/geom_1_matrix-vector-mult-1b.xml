<diagram dimensions="(300,300)" margins="5">
    <coordinates bbox="[-2.5,-1.5,6.5,5.5]">
        <axes at="axes" decorations="no" hticks="(-2,1,6)" vticks="(-1,1,5)" />
        <group at="vectors" outline="tactile">            
            <group at="vectory" >
                <vector at="vector2" v="(-1,1)"  stroke="black" />
                <label anchor="(-1,1)" tactile-offset="(0,9)" alignment="northwest" >
                    <m>\vec{y}</m>
                </label>
            </group>
            <group at="vectorAy" >
                <vector at="vector5" v="(3,1)"  stroke="black" />
                <label anchor="(3,1)" tactile-offset="(6,0)" alignment="east" >
                    <m>A\vec{y}</m>
                </label>
            </group>
        </group>
        

    </coordinates>

    <annotations>
    <annotation ref="figure"
                text="The two-dimensional plane with axes, vector y, and the result of multiplying matrix A by vector y">
        <annotation ref="axes" text="The coordinate axes" />
          <annotation ref="vectory" text="Vector y"/>
          <annotation ref="vectorAy" text="The vector that is the result of multiplying matrix A times vector y"/>
      </annotation>
  </annotations>
</diagram>