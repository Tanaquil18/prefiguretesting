<diagram dimensions="(300,200)" margins="5">
    <coordinates bbox="[-3,-1.5,3.5,2.0]">
        <axes at="axes" decorations="no" />
        <group at="vectors" outline="tactile">
            <group at="vectorx" >
                <vector at="vector1" v="(1,0.5)" stroke="black" />
                <label anchor="(1,0.6)" alignment="north">
                    <m>\vec{x}</m>
                </label>
            </group>
            <group at="vectory" >
                <vector at="vector2" v="(-1,1)" stroke="black" />
                <label anchor="(-1, 1.1)" alignment="north">
                    <m>\vec{y}</m>
                </label>
            </group>
            <group at="vectornegy" >
                <vector at="negvector2" v="(1,-1)" stroke="black" />
                <label anchor="(1, -1.1)" alignment="south">
                    <m>-\vec{y}</m>
                </label>
            </group>
            
            <vector at="vector4" v="(1,-1)" tail="(1,0.5)" stroke="gray" />
            <vector at="vector5" v="(1,0.5)" tail="(1,-1)" stroke="gray" />

            <group at="vectorxminusy" >
                <vector at="vector3" v="(2,-0.5)" stroke="black" dash="9 9" />
                <label anchor="(2.8, -0.3)" tactile-anchor="(2.6,-0.75)" alignment="south">
                    <m>\vec{x}-\vec{y}</m>
                </label>
            </group>
            <!-- <vector at="vector1times3" v="(3,3)" stroke="black" />
            <vector at="negativevector2" v="(2,-1)" stroke="black" /> -->
        </group>
        
        
        <!-- <label anchor="(2.9,3.1)" alignment="northwest" >
            <m>3\vec{x}</m>
        </label>
        <label anchor="(2,-0.8)" tactile-anchor="(2.5,-0.8)" alignment="north" >
            <m>-\vec{y}</m>
        </label> -->

    </coordinates>

    <annotations>
    <annotation ref="figure"
                text="The two-dimensional plane with axes, a vector x, a vector y, and the vector x minus vector y">
        <annotation ref="axes" text="The coordinate axes" />
        <!-- <annotation ref="vectors" text="Two vectors"> -->
          <annotation ref="vectorx" text="Vector x"/>
          <!-- <annotation ref="vector1times3" text="Three times vector x drawn pointing up 3 and to the right 3"/> -->
          <annotation ref="vectory" text="Vector y"/>
          <annotation ref="vectornegy" text="Vector negative y"/>

          <annotation ref="vector4" text="Vector negative y drawn with tail at the head of vector x"/>
          <annotation ref="vector5" text="Vector x drawn with tail at the head of vector negative y"/>
          <annotation ref="vectorxminusy" text="Vector x minus vector y" /> -->
        <!-- </annotation> -->
      </annotation>
  </annotations>
</diagram>