<diagram dimensions="(300,300)" margins="5">
    <coordinates bbox="[-2.5,-1.5,6.5,5.5]">
        <axes at="axes" decorations="no" hticks="(-2,1,6)" vticks="(-1,1,5)" />
        <group at="vectors" outline="tactile">
            <group at="vectorz" >
                <vector at="vector3" v="(3,-1)"  stroke="black" />
                <label anchor="(3,-1)" alignment="southeast" >
                    <m>\vec{z}</m>
                </label>
            </group>
            <group at="vectorAz" >
                <vector at="vector6" v="(-1,3)" stroke="black" />
                <!-- <label anchor="(-1,3)"   alignment="north" offset="(0,5)" tactile-offset="(-12,5)"> -->
                <label anchor="(-1,3)"   alignment="north" offset="(0,5)" tactile-anchor="(-1.5,3.2)">
                    <m>A\vec{z}</m>
                </label>
            </group>
        </group>
        

    </coordinates>

    <annotations>
    <annotation ref="figure"
                text="The two-dimensional plane with axes, vector z, and the result of multiplying matrix A by vector z">
        <annotation ref="axes" text="The coordinate axes" />
          <annotation ref="vectorz" text="Vector z"/>
          <annotation ref="vectorAz" text="The vector that is the result of multiplying matrix A times vector z"/>
      </annotation>
  </annotations>
</diagram>