<diagram dimensions="(300,200)" margins="5">
    <coordinates bbox="[-3,-0.5,3.5,1.5]">
        <axes at="axes" decorations="no" />
        <group at="vectors" outline="tactile">
            <group at="vectorx" >
                <vector at="vector1" v="(1,0.5)" stroke="black" />
                <label anchor="(1.1,0.5)" alignment="east">
                    <m>\vec{x}</m>
                </label>
            </group>
            <group at="vectory" >
                <vector at="vector2" v="(-1,1)" stroke="black" />
                <label anchor="(-1, 1.1)" alignment="north">
                    <m>\vec{y}</m>
                </label>
            </group>
            <!-- <vector at="vector1times3" v="(3,3)" stroke="black" />
            <vector at="negativevector2" v="(2,-1)" stroke="black" /> -->
        </group>
        
        
